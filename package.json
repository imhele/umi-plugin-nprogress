{
  "name": "umi-plugin-nprogress-monorepo",
  "private": true,
  "scripts": {
    "build": "wc-bundler",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "postinstall": "node npm-lifecycle/postinstall.js",
    "prepublishOnly": "npm run update:lockfile && npm run build && npm run test",
    "publish:all": "lerna publish --no-push && git push origin master",
    "test": "jest",
    "test:watch": "jest --coverage --watchAll",
    "test:ci": "jest --coverage --ci",
    "test:coverage": "jest --coverage",
    "update:lockfile": "pnpm i && git add pnpm-lock.yaml && git commit -m \"chore: update lockfile\""
  },
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/eslint-parser": "~7.15.0",
    "@babel/plugin-proposal-decorators": "~7.14.5",
    "@babel/plugin-proposal-do-expressions": "~7.14.5",
    "@babel/plugin-proposal-optional-chaining": "~7.14.5",
    "@babel/preset-env": "~7.15.0",
    "@jest/types": "^27.0.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.6",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chalk": "^4.1.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^2.1.0",
    "react": "^16.8.0 || ^17.0.0",
    "typescript": "^4.3.2",
    "wc-bundler": "^0.10.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "engines": {
    "node": ">=12",
    "pnpm": ">=6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imhele/umi-plugin-nprogress"
  },
  "homepage": "https://github.com/imhele/umi-plugin-nprogress",
  "author": "何乐 <work@imhele.com> (https://github.com/imhele)",
  "bugs": {
    "url": "https://github.com/imhele/umi-plugin-nprogress/issues"
  },
  "keywords": [
    "umi",
    "umijs",
    "plugin",
    "nprogress"
  ]
}
